{
    "plugins": [
        "@prettier/plugin-php",
        "@shufo/prettier-plugin-blade",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": [
                "*.blade.php"
            ],
            "options": {
                "parser": "blade",
                "tabWidth": 4
            }
        }
    ]
}
{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/react-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "althinect/filament-spatie-roles-permissions": "^2.3",
        "asmit/filament-mention": "^1.0",
        "bezhansalleh/filament-shield": "^3.3",
        "dotswan/filament-laravel-pulse": "^1.1",
        "filament/filament": "^3.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^2.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.0",
        "laravel/scout": "^10.13",
        "laravel/socialite": "^5.18",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.10.1",
        "meilisearch/meilisearch-php": "^1.13",
        "php-http/curl-client": "^2.3",
        "pinkary-project/type-guard": "^0.1.0",
        "predis/predis": "^2.3",
        "pxlrbt/filament-environment-indicator": "^2.1",
        "sentry/sentry-laravel": "^4.13",
        "shuvroroy/filament-spatie-laravel-health": "^2.3",
        "silviolleite/laravelpwa": "^2.0",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "tightenco/ziggy": "^2.4",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "aschmelyun/fleet": "^1.1",
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-type-coverage": "^3.4",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.0",
        "tightenco/duster": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse --memory-limit=256M",
        "test:arch": "pest --filter=arch",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test": [
            "@test:lint",
            "@test:refactor",
            "@test:types",
            "@test:type-coverage"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2.71",
        "genealabs/laravel-model-caching": "^11.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "intervention/image": "^3.6",
        "intervention/image-laravel": "^1.2",
        "laravel/framework": "^11.0",
        "laravel/passport": "^12.0",
        "laravel/pulse": "^1.0@beta",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.12",
        "laravel/socialite": "^5.12",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.4",
        "masmerise/livewire-toaster": "^2.6",
        "meilisearch/meilisearch-php": "^1.10",
        "php-http/curl-client": "^2.3",
        "predis/predis": "^2.2",
        "ralphjsmit/laravel-seo": "^1.6",
        "rappasoft/laravel-authentication-log": "^4.0",
        "sentry/sentry": "^4.8",
        "sentry/sentry-laravel": "^4.5",
        "tapp/filament-authentication-log": "^3.1",
        "torann/geoip": "^3.0",
        "typesense/typesense-php": "^4.9",
        "whichbrowser/parser": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.65",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.40",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "pestphp/pest-plugin-livewire": "^2.1",
        "recca0120/laravel-erd": "^0.1",
        "rector/rector": "^1.2",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:unit": "pest --parallel --coverage --min=100",
        "test": [
            "@test:lint",
            "@test:refactor",
            "@test:types",
            "@test:type-coverage",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
